#container{
 
}
#carSelect{
  margin: 20rpx 30rpx ;
  display: flex;
  padding: 30rpx 15rpx ;
  align-items: center;
  background: #fff;
}
.carSelect_right_bottom{
  display: flex;
  align-items: center;
}
.carSelect_right{
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}


.bottom {
    position: fixed;
    bottom: 0;
    width: 750rpx;
    height: 120rpx;
    border-top: 1rpx solid #ebedf0;
    background: rgba(255, 255, 255, 1);
    box-shadow: 0px 2rpx 0px 0px rgba(229, 229, 229, 1);
    padding: 0 40rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    z-index: 99;
}
button:not([size='mini']) {
    width: 400rpx;
    margin-left: auto;
    margin-right: auto;
    color: #fff;
    font-size: 34rpx;
}
