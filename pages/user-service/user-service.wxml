<view id="user_service">
  <view class="service_header">
    <view style="font-weight:700;font-size:40rpx;">
      我要咨询、反馈
    </view>
    <view class="service_img">
      <image src="/images/service1.png" style="height:150rpx;width:335rpx;margin-top:30rpx;"></image>
      <image src="/images/service2.png" style="height:150rpx;width:335rpx;margin-top:30rpx;"></image>
      <image src="/images/service3.png" style="height:150rpx;width:335rpx;margin:20rpx 0;"></image>
    </view>
  </view>
  <view style="font-weight:700;font-size:40rpx;">
    书享书联系方式
  </view>
  <view id="contact">
    <view class="contact" bindtap="getQQ">
      <image src="/images/qq.png" style="height:80rpx;;width:80rpx"></image>
      <view style="margin-top:15rpx;">客服QQ</view>
      <view > 119****0781</view>
      <view style="font-size:28rpx;color:#969696;margin-top:20rpx;">点击可复制QQ号</view>
    </view>
    <view class="contact" bindtap="getWX">
      <image src="/images/wx.png"style="height:80rpx;;width:80rpx"></image>
      <view style="margin-top:15rpx;">微信服务号</view>
      <view >we***2620781</view>
       <view style="font-size:28rpx;color:#969696;margin-top:20rpx;">点击可复制微信号</view>
    </view>
    <view class="contact" bindtap="getAD">
      <image src="/images/ad.png"style="height:80rpx;;width:80rpx"></image>
      <view style="margin-top:15rpx;">官方网址</view>
      <view >http:*****.cn</view>
       <view style="font-size:28rpx;color:#969696;margin-top:20rpx;">点击可复制网址号</view>
    </view>
  </view>
</view>